    <div class="carousel-products" data-flickity='{ "groupCells": true }'>
      <% Product.all.each do |product| %>
        <div class="carousel-cell">
            <%= link_to product_path(product) do %>
              <% if product.photo.present? %>
                <%= image_tag product.photo, id: "photo" %>
              <% else %>
                <%= image_tag "" %>
              <% end %>
            <% end %>
            <hr class="line">
            <div class="title">
              <h3><%= product.name.upcase %></h3>
            </div>
            <hr class="line">
            <div class="infos">
              <p><%= product.category.name %></p>
              <p>R$ <%= product.price %></p>
            </div>
        </div>
      <% end %>
